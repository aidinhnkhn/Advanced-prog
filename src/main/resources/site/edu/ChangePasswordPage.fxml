<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: CORNFLOWERBLUE;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ChangePasswordPage">
   <children>
      <VBox layoutX="130.0" layoutY="87.0" prefHeight="200.0" prefWidth="254.0" spacing="10.0">
         <children>
            <PasswordField fx:id="oldPassword" prefHeight="25.0" prefWidth="196.0" promptText="enter old password" />
            <PasswordField fx:id="newPassword" prefHeight="25.0" prefWidth="196.0" promptText="new password" />
            <PasswordField fx:id="confirmPassword" promptText="confirm password" />
            <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submit" prefHeight="25.0" prefWidth="262.0" text="submit" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
