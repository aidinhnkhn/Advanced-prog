<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: CORNFLOWERBLUE;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ProfessorRequestPage">
   <children>
      <TextArea fx:id="textArea" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="458.0" />
      <HBox layoutX="14.0" layoutY="426.0" prefHeight="100.0" prefWidth="458.0" spacing="40.0">
         <children>
            <Button fx:id="acceptButton" mnemonicParsing="false" onAction="#accept" prefHeight="25.0" prefWidth="80.0" text="Accept" />
            <Button fx:id="rejectButton" mnemonicParsing="false" onAction="#reject" prefWidth="80.0" text="Reject" />
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#goNext" prefWidth="80.0" text="Next " />
            <Button fx:id="previousButton" mnemonicParsing="false" onAction="#goBack" prefWidth="80.0" text="Previous" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <Button fx:id="homePageButton" layoutX="770.0" layoutY="561.0" mnemonicParsing="false" onAction="#HomePage" prefHeight="25.0" prefWidth="116.0" text="Home Page" />
      <ComboBox fx:id="requestBox" layoutX="490.0" layoutY="14.0" prefWidth="150.0" />
      <Button fx:id="showButton" layoutX="490.0" layoutY="55.0" mnemonicParsing="false" onAction="#show" prefHeight="25.0" prefWidth="150.0" text="set" />
   </children>
</AnchorPane>
