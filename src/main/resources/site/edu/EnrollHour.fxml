            <DatePicker fx:id="studentDate" promptText="pick a date" />
            <TextField fx:id="studentHour" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="174.0" promptText="enter hour" />
