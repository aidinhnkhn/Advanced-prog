               <font>
                  <Font name="Comic Sans MS" size="14.0" />
               </font>
            </CheckBox>
            <TextField fx:id="enterYear" />
            <CheckBox fx:id="degreeCheck" mnemonicParsing="false" text="Degree">
               <font>
                  <Font name="Comic Sans MS" size="14.0" />
               </font>
            </CheckBox>
            <ComboBox fx:id="degree" prefWidth="150.0" promptText="degree" />
